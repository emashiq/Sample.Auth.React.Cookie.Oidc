{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "oidc": {
    "Authority": "Authority",
    "ClientId": "OAuth2.0ClientId",
    "DefaultScopes": [
      "openid",
      "profile",
      "offline_access"
    ],
    "PostLogoutRedirectUri": "/",
    "ResponseType": "code"
  },
  "OidcConfig": {
    "PageTitle": "React Sample",
    "FaviconUri": "~/favicon.ico",
    // For Docker Enable Host
    "DockerHost": "http://smartfactoryidentityserver.sts.identity/",
    "IsDocker": true,
    "IdentityAdminRedirectUri": "https://localhost:44395/signin-oidc",
    "IdentityServerBaseUrl": "https://localhost:44310/",
    "IdentityAdminCookieName": "IdentityServerAdmin",
    "IdentityAdminCookieExpiresUtcHours": 12,
    "RequireHttpsMetadata": false,
    "TokenValidationClaimName": "name",
    "TokenValidationClaimRole": "role",
    "ClientId": "ReactAppTest",
    "ClientSecret": "Secret",
    "OidcResponseType": "code",
    "Scopes": [
      "openid",
      "profile",
      "offline_access"
    ],
    "AdministrationRole": "MyRole",
    "HideUIForMSSqlErrorLogging": false
  },
  "AllowedHosts": "*"
}
